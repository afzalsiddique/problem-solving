    "n=int(input())\n",
    "length=[int(x) for x in input().split()]\n",
    "\n",
    "myset=set(length)\n",
    "\n",
    "occurence=[]\n",
    "for item in myset:\n",
    "    occurence.append(len([x for x in length if x!=item]))\n",
    "occurence=[n-x for x in occurence]\n",
    "print(max(occurence), len(myset))"
   ]
  }
