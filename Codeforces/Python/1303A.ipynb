    "n=int(input())\n",
    "for i in range(n):\n",
    "    s=input()\n",
    "    l=len(s)\n",
    "    first=s.find('1')\n",
    "    last=l-s[::-1].find('1')-1\n",
    "    if first==-1:\n",
    "        print(0)\n",
    "    else:\n",
    "        count=0\n",
    "        for i in range(first,last):\n",
    "            if s[i]=='0':\n",
    "                count+=1\n",
    "        print(count)"
   ]
